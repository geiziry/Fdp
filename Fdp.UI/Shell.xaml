<dx:DXWindow x:Class="Fdp.UI.Shell"
             xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
             xmlns:dxt="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
             xmlns:dxc="http://schemas.devexpress.com/winfx/2008/xaml/core"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:local="clr-namespace:Fdp.UI"
             xmlns:Strings="clr-namespace:Fdp.InfraStructure;assembly=Fdp.InfraStructure"
             xmlns:prism="http://prismlibrary.com/"
             mc:Ignorable="d"
             Title="Fdp"
             Height="350"
             Width="1200"
             ResizeMode="CanResizeWithGrip">
    <dx:DXWindow.Resources>

    </dx:DXWindow.Resources>
    <Grid>
        <DockPanel LastChildFill="True">
            <ContentControl prism:RegionManager.RegionName="{x:Static Strings:Strings.MenuRegion}"
                            Height="30"
                            DockPanel.Dock="Top" />
            <ContentControl prism:RegionManager.RegionName="{x:Static Strings:Strings.MainRegion}" />
        </DockPanel>
    </Grid>
</dx:DXWindow>
